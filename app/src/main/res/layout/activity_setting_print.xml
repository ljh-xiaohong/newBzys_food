<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/choose_layout"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="#ff9c00">
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/layout_title_bar"
        android:layout_width="match_parent"
        android:layout_height="180px"
        android:background="#ff9c00">
        <ImageButton
            android:layout_width="69px"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:onClick="back"
            android:paddingLeft="30px"
            android:src="@mipmap/btn_back"
            android:background="@null"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"
            android:textColor="@android:color/white"
            android:textSize="51px"
            android:layout_centerInParent="true"/>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_height="match_parent">
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/lay_print"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="下单后自动打印"
            android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/print_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/lay_set_print_count"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_below="@+id/print_switch"
            android:orientation="vertical"
            android:layout_height="wrap_content">
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/lay_money_print"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:id="@+id/print_money_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_height="wrap_content"
                />
            <TextView
                android:layout_toEndOf="@id/print_money_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="收银打印"/>
            <ImageView
                android:id="@+id/img_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:src="@mipmap/food_press" />
            <TextView
                android:id="@+id/tv_money_count"
                android:layout_toStartOf="@id/img_down"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1分"/>
        </com.zhy.autolayout.AutoRelativeLayout>
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/lay_cook_all_print"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:id="@+id/print_cook_all_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_height="wrap_content"
                />
            <TextView
                android:layout_toEndOf="@id/print_cook_all_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="后厨打印总单"/>
            <ImageView
                android:id="@+id/img_down1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:src="@mipmap/food_press" />
            <TextView
                android:id="@+id/tv_cook_all_count"
                android:layout_toStartOf="@id/img_down1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1分"/>
        </com.zhy.autolayout.AutoRelativeLayout>
        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/lay_cook_one_print"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <CheckBox
                android:id="@+id/print_one_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_height="wrap_content"
                />
            <TextView
                android:layout_toEndOf="@id/print_one_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="后厨打印分单"/>
            <ImageView
                android:id="@+id/img_down2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:src="@mipmap/food_press" />
            <TextView
                android:id="@+id/tv_cook_one_count"
                android:layout_toStartOf="@id/img_down2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1分"/>
        </com.zhy.autolayout.AutoRelativeLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/lay_end_cook_print"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="点击下厨后打印"
            android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/lay_end_cook_print_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/lay_end_cook_count"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_below="@+id/lay_end_cook_print_switch"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_end_cook_money_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_end_cook_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_end_cook_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="收银打印"/>
                <ImageView
                    android:id="@+id/img_down3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_end_cook_count"
                    android:layout_toStartOf="@id/img_down3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_end_cook_cook_all_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_end_cook_cook_all_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_end_cook_cook_all_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="后厨打印总单"/>
                <ImageView
                    android:id="@+id/img_down4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_end_cook_cook_all_count"
                    android:layout_toStartOf="@id/img_down4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_end_cook_cook_one_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_end_cook_one_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_end_cook_one_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="后厨打印分单"/>
                <ImageView
                    android:id="@+id/img_down5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_end_cook_cook_one_count"
                    android:layout_toStartOf="@id/img_down5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/lay_pay_print"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="点击收款后打印"
            android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/lay_pay_print_switch"
                android:layout_width="wrap_content"
                android:background="@drawable/cb_select"
                android:button="@null"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/lay_pay_count"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_below="@+id/lay_pay_print_switch"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_pay_money_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_pay_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_pay_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="收银打印"/>
                <ImageView
                    android:id="@+id/img_down6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_pay_count"
                    android:layout_toStartOf="@id/img_down6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_pay_cook_all_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_pay_cook_all_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_pay_cook_all_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="后厨打印总单"/>
                <ImageView
                    android:id="@+id/img_down7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_pay_cook_all_count"
                    android:layout_toStartOf="@id/img_down7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
            <com.zhy.autolayout.AutoRelativeLayout
                android:id="@+id/lay_pay_cook_one_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <CheckBox
                    android:id="@+id/print_pay_one_switch"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cb_select"
                    android:button="@null"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:layout_toEndOf="@id/print_pay_one_switch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="后厨打印分单"/>
                <ImageView
                    android:id="@+id/img_down8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@mipmap/food_press" />
                <TextView
                    android:id="@+id/tv_pay_cook_one_count"
                    android:layout_toStartOf="@id/img_down8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1分"/>
            </com.zhy.autolayout.AutoRelativeLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoRelativeLayout>
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>